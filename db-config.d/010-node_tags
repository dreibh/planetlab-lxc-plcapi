# -*-python-*-
#################### node tag types

# Setup default node tag types
nodetag_types = \
[
    # Node HRN (SFA specific human readable name)
    {'tagname': 'hrn',
     'description': 'SFA human readable name',
     'category' : 'node/sfa',
     'roles' : ['admin','pi','user','tech']},
]

for nodetag_type in nodetag_types:
    SetTagType(nodetag_type)
